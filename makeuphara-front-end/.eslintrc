{
  "extends": ["react-app", "react-app/jest", "airbnb", "prettier"],
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-experssion-per-line": 0,
    "no-shadow": 0,
    "camelcase": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0
  },
  "env": { "browser": true }
}
